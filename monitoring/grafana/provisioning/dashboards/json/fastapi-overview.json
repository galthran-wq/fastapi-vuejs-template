{
  "annotations": {"list": []},
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "stat",
      "title": "Active Requests",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "http_requests_active", "legendFormat": "active"}
      ]
    },
    {
      "type": "graph",
      "title": "Requests Total",
      "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
      "targets": [
        {"expr": "sum(rate(http_requests_total[5m])) by (method, endpoint, status_code)", "legendFormat": "{{method}} {{endpoint}} {{status_code}}"}
      ],
      "lines": true,
      "fill": 1
    },
    {
      "type": "graph",
      "title": "Request Duration (p95)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, method, endpoint))", "legendFormat": "{{method}} {{endpoint}} p95"}
      ],
      "lines": true,
      "fill": 1
    }
  ],
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["fastapi", "prometheus"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
  "timepicker": {},
  "timezone": "",
  "title": "FastAPI Overview",
  "version": 1
}

